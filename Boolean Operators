public class Operations {
    public static void main (String[] args) {

        // modulus or remainder
        int a = 11;
        int b = 5;
        int c = a % b;


        // modulus or remainder
        int a = 11;
        int b = 5;
        int c = a % b;

// LOGICAL OPERATORS
        // negation
        boolean d = true;
        boolean e = !d;

        // AND - returns true when both are true
        boolean f = true;
        boolean g = true;
        boolean h = f && g;

        // OR - returns true if one is true
        boolean i = false;
        boolean j = true;
        boolean k = i || j;

        // exclusive OR - returns true is one is true and another false
        boolean l = true;
        boolean m = false;
        boolean n = l ^ m;

// RELATIONAL OPERATORS
          // EQUALITY - primitive and reference data types (Returns true is the operators or objects are the same respectively

        int o = 1, p = 1;
        boolean q = o == p;

        String r = "true", s = "false";
        boolean t = r == s;

 //  NOT EQUAL
        int v = 1, w = 1;
        boolean x = v != w;

        String y = "true", z = "false";
        boolean a1 = y != z;


        //  NOT EQUAL
        int v = 1, w = 1;
        boolean x = v != w;

        String y = "true", z = "false";
        boolean a1 = y != z;

        
  

    }

}
